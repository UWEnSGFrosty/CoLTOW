    <selectionEntry id="e929-45af-82c6-7a67" name="Temple Guard" hidden="false" collective="false" import="true" type="unit">
      <constraints>
        <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="320f-2067-5caf-e987" type="max"/>
