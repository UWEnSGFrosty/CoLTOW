          <profiles>
            <profile id="c2ef-62b5-700d-5f30" name="Diabolic Splendour" publicationId="b907-f7a4-2802-2a2e" page="77" hidden="false" typeId="417263616e6520616e6420456e6368616e746564204974656d23232344415441232323" typeName="Arcane and Enchanted Item">
