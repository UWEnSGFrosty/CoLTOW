        <selectionEntryGroup id="9347-5d42-a96a-4b60" name="Bloody Gifts" hidden="false" collective="false" import="true">
          <selectionEntries>
            <selectionEntry id="6173-7377-2b45-6057" name="Cry of War" hidden="false" collective="false" import="true" type="upgrade">
              <constraints>
                <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="4455-338a-4298-7a07" type="max"/>
              </constraints>
              <rules>
                <rule id="d517-b104-c2fb-c659" name="Cry of War" publicationId="b826-f500-02b1-2f5d" page="6" hidden="false">
                  <description>Enemy units suffer a -1 modifier to their Leadership characteristic whilst within the Command range of one or more Death Hags that have this special rule and that are not fleeing.</description>
                </rule>
              </rules>
              <costs>
                <cost name="pts" typeId="points" value="15.0"/>
              </costs>
            </selectionEntry>
            <selectionEntry id="de50-68df-e8ed-c53b" name="Dance of Doom" hidden="false" collective="false" import="true" type="upgrade">
              <constraints>
                <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="8c6e-1627-d5fb-7710" type="max"/>
              </constraints>
              <rules>
                <rule id="f9b0-b053-8acf-4b71" name="Dance of Doom" page="" hidden="false">
                  <description>This model has a 5+ Ward Save against non-Magical Attacks</description>
                </rule>
              </rules>
              <costs>
                <cost name="pts" typeId="points" value="20.0"/>
              </costs>
            </selectionEntry>
            <selectionEntry id="06c8-67b4-941f-dbb7" name="Deathtouch" hidden="false" collective="false" import="true" type="upgrade">
              <constraints>
                <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="8ade-f0f2-f6a6-8e09" type="max"/>
              </constraints>
              <rules>
                <rule id="4418-7f3d-cc58-66c6" name="Deathtouch" page="" hidden="false">
                  <description>This character gains Killing Blow. While wielding a weapon that has Killing Blow, the weapon instead has Monster Slayer.</description>
                </rule>
              </rules>
              <infoLinks>
                <infoLink id="bae3-3645-aae3-9a8f" name="Killing Blow" hidden="false" targetId="f476-ed24-c451-6e04" type="rule"/>
              </infoLinks>
              <costs>
                <cost name="pts" typeId="points" value="35.0"/>
              </costs>
            </selectionEntry>
            <selectionEntry id="c8b4-bf1f-450b-61e5" name="Hand of Khaine" hidden="false" collective="false" import="true" type="upgrade">
              <constraints>
                <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="5498-b6a2-bdf6-53fa" type="max"/>
              </constraints>
              <rules>
                <rule id="6f5a-6630-1344-4faa" name="Hand of Khaine " page="" hidden="false">
                  <description>The Character chooses one model in base contact to lose one Attack. Against mounted models, they must choose either the mount or the rider. If the model has several types of attack (an extra bite, for example) they must choose which type of attack is lost. Models which are Immune to Psychology are not affected by the Hand of Khaine.</description>
                </rule>
              </rules>
              <costs>
                <cost name="pts" typeId="points" value="25.0"/>
              </costs>
            </selectionEntry>
            <selectionEntry id="7c5d-3a33-9928-134b" name="Rune of Khaine" hidden="false" collective="false" import="true" type="upgrade">
              <constraints>
                <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="dd89-a177-46c9-e528" type="max"/>
              </constraints>
              <rules>
                <rule id="669e-eeae-0948-9a39" name="Rune of Khaine" publicationId="b826-f500-02b1-2f5d" page="6" hidden="false">
                  <description>This character has the Extra Attacks (+D3) special rule.</description>
                </rule>
              </rules>
              <infoLinks>
                <infoLink id="10a9-cfb2-4d24-5694" name="Extra Attacks (+X)" hidden="false" targetId="3f29-140b-ef76-192f" type="rule">
                  <modifiers>
                    <modifier type="set" field="name" value="Extra Attacks (+D3)"/>
                  </modifiers>
                </infoLink>
              </infoLinks>
              <costs>
                <cost name="pts" typeId="points" value="10.0"/>
              </costs>
            </selectionEntry>
            <selectionEntry id="4799-5d44-b897-3dda" name="Witchbrew" hidden="false" collective="false" import="true" type="upgrade">
              <constraints>
                <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="248d-679d-c22c-1196" type="max"/>
              </constraints>
              <rules>
                <rule id="c538-c936-7d76-4266" name="Witchbrew" hidden="false">
                  <description>This character, their mount and any unit they have joined cannot lose the Frenzy special rule.</description>
                </rule>
